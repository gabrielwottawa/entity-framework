@model IEnumerable<MusicMVC.Models.Artists_Genres>

@{
    ViewBag.Title = "Artist/Genre List";
}

<h2>Artist/Genre List</h2>

<p>
    @Html.ActionLink("Add New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Artist.ArtistName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Genres.GenreName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Artist.ArtistName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Genres.GenreName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Artists_GenresID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Artists_GenresID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Artists_GenresID })
        </td>
    </tr>
}

</table>
